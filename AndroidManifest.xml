<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vantagevtrcu"
    android:installLocation="auto"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/vt_icon_final"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen" android:permission="android.permission.INTERNET" android:debuggable="false">
        <activity
            android:name="com.vantagevtrcu.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.vantagevtrcu.settingsActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.vantagevtrcu.numbersActivity"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>